					"LSDatabaseName": {
						"type": "string"
					},
					"LSUsername": {
						"type": "string"
					},
					"LSPassword": {
						"type": "string"
					}
				},
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('Ingest_LS_SQLServer_SQLAuth_connectionString')]"
				}
			},
			"dependsOn": []
