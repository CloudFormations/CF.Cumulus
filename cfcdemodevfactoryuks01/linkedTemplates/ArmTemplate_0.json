				"typeProperties": {
					"url": "[parameters('Ingest_LS_HTTP_AnonAuth_properties_typeProperties_url')]",
					"enableServerCertificateValidation": true,
					"authenticationType": "Anonymous"
				}
			},
			"dependsOn": []
