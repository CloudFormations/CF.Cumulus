{
	"name": "Ingest_LS_AzurePostgreSQL_SPAuth",
	"properties": {
		"parameters": {
			"LSServicePrincipalKey": {
				"type": "String"
			},
			"LSHostName": {
				"type": "String"
			},
			"LSPortNumber": {
				"type": "String"
			},
			"LSDatabaseName": {
				"type": "String"
			},
			"LSServicePrincipalName": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "AzurePostgreSql",
		"version": "1.0",
		"typeProperties": {
			"connectionString": "Host=@{linkedService().LSHostName};Port=@{linkedService().LSPortNumber};Database=@{linkedService().LSDatabaseName};UID=@{linkedService().LSServicePrincipalName};EncryptionMethod=0",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Common_LS_cumuluskeys",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().LSServicePrincipalKey",
					"type": "Expression"
				}
			}
		}
	}
}