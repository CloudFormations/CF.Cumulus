{
	"name": "Ingest_LS_HTTP_AnonAuth",
	"properties": {
		"parameters": {
			"BaseURL": {
				"type": "string"
			},
			"Authorization": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{linkedService().BaseURL}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous",
			"authHeaders": {
				"Authorization": {
					"type": "AzureKeyVaultSecret",
					"store": {
						"referenceName": "Common_LS_cumuluskeys",
						"type": "LinkedServiceReference"
					},
					"secretName": {
						"value": "@linkedService().Authorization",
						"type": "Expression"
					}
				}
			}
		}
	}
}