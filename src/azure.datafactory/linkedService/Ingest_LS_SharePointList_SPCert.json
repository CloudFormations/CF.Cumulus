			"SiteURL": {
				"type": "string"
			},
			"ServicePrincipalId": {
				"type": "string"
			},
			"ServicePrincipalCertificateSecretName": {
				"type": "string"
			},
			"ServicePrincipalCertificatePasswordSecretName": {
				"type": "string"
			},
			"TenantId": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "SharePointOnlineList",
