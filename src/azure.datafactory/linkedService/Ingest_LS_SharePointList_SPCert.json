				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Common_LS_cumuluskeys",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().ServicePrincipalCertificateSecretName",
					"type": "Expression"
				}
			},
			"servicePrincipalEmbeddedCertPassword": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "Common_LS_cumuluskeys",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().ServicePrincipalCertificatePasswordSecretName",
					"type": "Expression"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}