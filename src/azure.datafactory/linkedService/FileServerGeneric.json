{
	"name": "FileServerGeneric",
	"properties": {
		"parameters": {
			"Host": {
				"type": "string",
				"defaultValue": "C:\\SourceData\\"
			},
			"UserName": {
				"type": "string",
				"defaultValue": "azure_vm_admin"
			},
			"Password": {
				"type": "string",
				"defaultValue": "vmadminpassword"
			}
		},
		"annotations": [],
		"type": "FileServer",
		"typeProperties": {
			"host": "@{linkedService().Host}",
			"userId": "@{linkedService().UserName}",
			"password": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "cumuluskeys",
					"type": "LinkedServiceReference"
				},
				"secretName": {
					"value": "@linkedService().Password",
					"type": "Expression"
				}
			}
		},
		"connectVia": {
			"referenceName": "datasourceVM02",
			"type": "IntegrationRuntimeReference"
		}
	}
}