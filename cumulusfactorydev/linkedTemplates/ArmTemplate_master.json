{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "cumulusfactorydev"
		},
		"cumulusdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'cumulusdatabase'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=cumulussqlinstancedev.database.windows.net;Initial Catalog=cumulusdatabasedev"
		},
		"cumulusfunctions_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://cumulusfunctionappdev.azurewebsites.net"
		},
		"cumuluskeys_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://cumuluskeysdev.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "cumulusfactorydev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"cumulusdatabase_connectionString": {
						"value": "[parameters('cumulusdatabase_connectionString')]"
					},
					"cumuluskeys_properties_typeProperties_baseUrl": {
						"value": "[parameters('cumuluskeys_properties_typeProperties_baseUrl')]"
					},
					"cumulusfunctions_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('cumulusfunctions_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}